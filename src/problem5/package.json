{
  "name": "crude",
  "version": "1.0.0",
  "description": "Crude application",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "db:local": "docker-compose --env-file .env up postgres -d",
    "prestart": "npx prisma db pull && npx prisma generate",
    "start": "NODE_ENV=production node dist/src/index.js",
    "start:dev": "yarn prestart && nodemon src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "seed": "ts-node prisma/seed/seed.ts",
    "migrate:generate": "npx prisma migrate dev --create-only --name",
    "migrate:latest": "npx prisma migrate deploy",
    "migrate": "npx prisma migrate resolve --applied"
  },
  "author": "Jamie Le",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "6.4.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "joi": "^17.13.3",
    "pino-http": "^10.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/joi": "^17.2.3",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  }
}